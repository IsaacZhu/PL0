var a[3][3][4],i,result,b[3][3][4];
procedure p1(a[3][3][4]);
	procedure p2(a[3][3][4]);
	begin
		i:=b[1][1][1];	//b[1][1][1] 1:0 2:4
		return i;
	end;
begin
	a[1][1][1]:=4;		//1:4 2:4

	result:=p2(a);	//b[1][1][1] //1:0 2:4
	return a[2][1][1];
end;
begin
	a[2][1][1]:=10;		//10
	b[2][1][1]:=7;		//7

	result:=p1(a);	//10
	i:=a[1][1][1];		//4

	result:=p1(b);	//7
	i:=b[1][1][1];		//4
end.
