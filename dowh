var result,a,i;
begin
        i:=0;
	result:=5;
        do
        {
           if(a==3){
                      do
                      {
                        if(i==2){
                              break;
                        }
                        i++;
                      }while(i<a);
           }//if
           a++;
        }while(a<result);
        printf("@0 @1\n", a,i);
end.
